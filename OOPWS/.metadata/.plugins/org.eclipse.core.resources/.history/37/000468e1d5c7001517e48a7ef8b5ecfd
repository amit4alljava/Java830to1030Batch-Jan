class W
{
	int x ;
	W(){
		x = 1000;
		System.out.println("W Class Default Cons X "+x);
	}
}
class P extends W
{
	int y ;
	P(){
		y = x + 200;
		System.out.println("P Class Default Cons Y "+y);
	}
}
public class ConstructorISA {

	public static void main(String[] args) {
		P obj = new P();
		

	}

}
